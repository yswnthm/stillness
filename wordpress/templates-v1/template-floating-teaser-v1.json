-family: 'Lato', sans-serif; font-size: 12px; letter-spacing: 2px; text-transform: uppercase; width: fit-content; } .teaser-text-col h2 { font-family: 'Cormorant Garamond', serif; font-size: 60px; line-height: 1; color: #F7F0EC; margin: 0; } .teaser-desc { font-family: 'Lato', sans-serif; font-size: 18px; line-height: 1.6; color: #A4B2BA; margin: 0; max-width: 450px; } .teaser-stats { display: flex; gap: 16px; align-items: flex-end; } .t-stat { text-align: center; } .t-num { display: block; font-family: 'Cormorant Garamond', serif; font-size: 30px; color: #F7F0EC; } .t-lbl { font-family: 'Lato', sans-serif; font-size: 10px; text-transform: uppercase; letter-spacing: 2px; color: #A4B2BA; } .t-sep { font-family: 'Cormorant Garamond', serif; font-size: 24px; color: rgba(164,178,186,0.3); padding-bottom: 8px; } .teaser-form-card { background: rgba(29, 49, 82, 0.3); backdrop-filter: blur(12px); border: 1px solid rgba(255,255,255,0.05); border-radius: 24px; padding: 48px; } .teaser-form-card h3 { font-family: 'Cormorant Garamond', serif; font-size: 24px; color: #F7F0EC; margin: 0 0 8px; } .teaser-form-card > p { font-family: 'Lato', sans-serif; font-size: 14px; color: #A4B2BA; margin: 0 0 24px; } .f-group { margin-bottom: 16px; } .f-group input, .f-group select { width: 100%; box-sizing: border-box; background: rgba(247,240,236,0.03); border: 1px solid rgba(247,240,236,0.1); border-radius: 8px; padding: 12px 16px; font-family: 'Lato', sans-serif; font-size: 14px; color: rgba(247,240,236,0.6); outline: none; transition: border-color 0.3s ease; } .f-group input:focus, .f-group select:focus { border-color: rgba(247,240,236,0.3); } .f-group select { appearance: none; } .teaser-submit { display: block; width: 100%; padding: 12px; background: rgba(247,240,236,0.1); backdrop-filter: blur(4px); border: 1px solid rgba(247,240,236,0.3); border-radius: 99px; color: #F7F0EC; font-family: 'Lato', sans-serif; font-size: 14px; font-weight: 700; text-transform: uppercase; letter-spacing: